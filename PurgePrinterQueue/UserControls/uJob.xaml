<UserControl x:Class="ResetPrinterQueue.UserControls.uJob"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ResetPrinterQueue.UserControls"
             mc:Ignorable="d" Height="60" Background="#CC119EDA" Width="600" Margin="1">
    <Canvas Margin="3" Background="White">
        <Image Height="40" Canvas.Left="10" Canvas.Top="7" Width="40" Source="/PurgePrinterQueue;component/Resources/icon8Printer.png" HorizontalAlignment="Left" VerticalAlignment="Center" />
        <Label x:Name="lblDocumentName" Content="Document Name" Canvas.Left="59" Background="{x:Null}" Canvas.Top="2" />
        <Label x:Name="lblDocumentInformation" Content="Printer Information" Canvas.Left="59" Canvas.Top="25" Background="{x:Null}" />
    </Canvas>
</UserControl>